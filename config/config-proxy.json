{
  "browsertime": {
    "browser": "firefox",
    "iterations": 2,
    "spa": true,
    "preScript": "root/script/proxy/proxy_start.js",
    "postScript": "root/script/proxy/proxy_end.js",
    "summary": true,
    "firefox.disableSafeBrowsing": true,
    "firefox.acceptInsecureCerts": true,
    "chrome.ignoreCertificateErrors": true,
    "firefox.args": [],
    "firefox.preference": {},
    "chrome.args": []
  },
  "proxy.https": "127.0.0.1:22",
  "proxy.http": "127.0.0.1:22",
  "html":{
    "showAllWaterfallSummary": true,
    "compareUrl": true
  },
  "Text": {
    "summary": false
  },
  "Sustainable": {
    "enable": true,
    "pageViews": 7
  },
  "axe":{
    "enable": true
  },
  "metrics":{
    "list": true,
    "filterList": true 
  },
  "gzipHAR": false,
  "video": true,
  "logToFile": true 
}
